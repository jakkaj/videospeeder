{
    "inputs": [],
    "servers": {
        "memory": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "--init",
                "-v",
                "/workspaces/mcp-knowledge-graph-improved/.roo:/data",
                "jakkaj/mcp-knowledge-graph",
                "node",
                "dist/index.js",
                "--server",
                "--memory-path",
                "/data/memory.jsonl"
            ]
        },
        "github": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "--env-file",
                "/workspaces/mcp-knowledge-graph-improved/.devcontainer/devcontainer.env",
                "ghcr.io/github/github-mcp-server"
            ],
            "env": {}
        }
    }
}